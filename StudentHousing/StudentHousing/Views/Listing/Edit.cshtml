@model StudentHousing.CompositeModel.ListingEditModel
@{
    ViewData["Title"] = "Create";
}

<h2>Create</h2>

<form method="post" enctype="multipart/form-data">
    <div>
        <label asp-for="Listing.Name"></label>
        <input asp-for="Listing.Name" />
        <span asp-validation-for="Listing.Name"></span>
    </div>
    <div>
        <label asp-for="Listing.Description"></label>
        <input asp-for="Listing.Description" />
        <span asp-validation-for="Listing.Description"></span>
    </div>
    <div>
        <label asp-for="Images"></label>
        <input type="file" id="file" asp-for="Images" multiple/>
        <span asp-validation-for="Images"></span>
    </div>


    <input type="submit" name="save" value="Save" />
</form>


